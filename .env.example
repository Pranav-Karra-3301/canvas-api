# Canvas API Configuration
# Copy this file to .env and fill in your values

# Your Canvas instance API URL (include /api/v1)
CANVAS_API_URL=https://your-canvas-instance.instructure.com/api/v1

# Your Canvas API token (from Account > Settings > New Access Token)
CANVAS_API_TOKEN=your_token_here

# Optional: Enable debug logging for rate limiter troubleshooting
# CANVAS_API_DEBUG=true
